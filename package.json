{
  "name": "nextjs-mdx-blog-starter",
  "description": "A blog to learn JavaScript, React.js, Node.js, Next.js and much more. By- Darshan Dhabale",
  "version": "0.2.4",
  "scripts": {
    "dev": "next",
    "dev-https": "local-ssl-proxy --source 3001 --target 3000 next",
    "prebuild": "node ./src/buildTools/buildSitemap.mjs",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format-watch": "npm run format && onchange \"**/*.{js,jsx,json,md}\" -- prettier --write {{changed}}",
    "deploy": "git push origin main && vercel --prod"
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@fullpage/react-fullpage": "^0.1.26",
    "@mdx-js/loader": "^2.0.0",
    "@mdx-js/runtime": "^1.6.22",
    "@mui/material": "^5.2.5",
    "@next/mdx": "^11.1.0",
    "disqus-react": "^1.0.10",
    "fs": "^0.0.1-security",
    "fullpage.js": "^4.0.9",
    "gray-matter": "^4.0.2",
    "gsap": "^3.11.3",
    "local-ssl-proxy": "^1.3.0",
    "next": "13.5.0",
    "ngrok": "^4.3.0",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-toastify": "^9.0.5",
    "sass": "^1.57.0",
    "theme-ui": "^0.10.0"
  },
  "devDependencies": {
    "onchange": "^7.1.0",
    "prettier": "^2.2.1"
  }
}
